<Window x:Class="Global_Domination.AccountSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Global_Domination"
        mc:Ignorable="d"
        Title="AccountSettings" Height="300" Width="300">
    <Grid>
        <Button x:Name="signOutButton" Content="Sign Out" HorizontalAlignment="Left" Margin="105,70,0,0" VerticalAlignment="Top" Width="75" Click="signOutButton_Click"/>
        <Button x:Name="settingsButton" Content="Settings" HorizontalAlignment="Left" Margin="105,134,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="105,195,0,0" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" />

    </Grid>
</Window>
